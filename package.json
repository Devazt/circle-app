      "reflect-metadata": "^0.1.13",
      "stream": "^0.0.2",
      "swagger-ui-express": "^5.0.0",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "nodemon --exec ts-node src/index.ts",
      "worker": "nodemon --exec ts-node src/worker/workerHub.ts",
