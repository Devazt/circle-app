      "typescript": "4.5.2"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "cloudinary": "^1.41.1",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "express-session": "^1.17.3",
      "joi": "^17.11.0",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.5-lts.1",
